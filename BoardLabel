import javax.swing.*;

public class BoardLabel extends JLabel {
    private static final long serialVersionUID = 1L;
    private final int i;
    private final int j;

    public BoardLabel(int i, int j) {
        super("");
        this.i = i;
        this.j = j;
    }

    public int getI() {
        return i;
    }

    public int getJ() {
        return j;
    }
}
